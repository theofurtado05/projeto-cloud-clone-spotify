generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model users {
  id                Int       @id @default(autoincrement())
  email             String    @unique @db.VarChar(255)
  password          String    @db.VarChar(500)
  phone             String    @db.VarChar(15)
  name              String    @db.VarChar(50)
}

